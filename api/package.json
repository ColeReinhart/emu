{
  "name": "emu-api",
  "version": "1.0.0",
  "description": "Emu RESTful API",
  "main": "app.js",
  "scripts": {
    "start": "node ./app.js",
    "start-watch": "nodemon --watch . --ignore ./__tests__ -x \"npm start\"",
    "dev": "npm run start-watch",
    "dev-postgres": "npm run db-migrate && npm run start-watch",
    "db-create": "./node_modules/.bin/sequelize db:create",
    "db-migrate": "./node_modules/.bin/sequelize db:migrate",
    "db-seed": "./node_modules/.bin/sequelize db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "@juaning",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "bcrypt-promise": "^2.0.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "mongoose": "^5.1.3",
    "mongoose-validator": "^2.0.2",
    "morgan": "^1.9.0",
    "parse-error": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "validator": "^10.2.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "nodemon": "^1.17.5"
  }
}
