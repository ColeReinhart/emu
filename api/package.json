{
  "name": "emu-api",
  "version": "1.0.0",
  "description": "Emu RESTful API",
  "main": "app.js",
  "scripts": {
    "start": "node ./app.js",
    "start-watch": "nodemon --watch . --ignore ./__tests__ -x \"npm start\"",
    "dev": "npm run db-migrate && npm run start-watch",
    "db-create": "./node_modules/.bin/sequelize db:create",
    "db-migrate": "./node_modules/.bin/sequelize db:migrate",
    "db-seed": "./node_modules/.bin/sequelize db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "@juaning",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.8",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "nodemon": "^1.17.5"
  }
}
